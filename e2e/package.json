{
  "name": "set-e2e",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "lint": "eslint 'tests/**/*.ts{,x}'",
    "playwright-local": "BASE_URL=http://localhost:8080 playwright test",
    "playwright-local:win": "set BASE_URL=http://localhost:8080& playwright test",
    "playwright-local-ui": "BASE_URL=http://localhost:8080 playwright test --ui",
    "playwright-local-ui:win": "set BASE_URL=http://localhost:8080& playwright test --ui",
    "playwright-local-debug": "BASE_URL=http://localhost:8080 playwright test --debug",
    "playwright-local-debug:win": "set BASE_URL=http://localhost:8080& playwright test --debug",
    "playwright-dev": "BASE_URL=https:// playwright test",
    "playwright-dev:win": "set BASE_URL=https://& playwright test",
    "playwright-dev-ui": "BASE_URL=https:// playwright test --ui",
    "playwright-dev-ui:win": "set BASE_URL=https://& playwright test --ui",
    "playwright-dev-debug": "BASE_URL=https:// playwright test --debug",
    "playwright-dev-debug:win": "set BASE_URL=https://& playwright test --debug",
    "playwright-test": "BASE_URL=https:// playwright test",
    "playwright-test:win": "set BASE_URL=https://& playwright test",
    "playwright-test-ui": "BASE_URL=https:// playwright test --ui",
    "playwright-test-ui:win": "set BASE_URL=https://& playwright test --ui",
