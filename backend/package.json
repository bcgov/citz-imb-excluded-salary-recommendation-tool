{
  "name": "set-backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon -L --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node ./dist/src/index.js",
    "test": "node --import tsx --test src/**/*.test.*",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js ",
    "migration:generate": "npm run typeorm -- migration:generate -d src/dataSource.ts",
    "migration:run": "npm run build && npm run typeorm -- migration:run -d src/dataSource.ts",
    "migration:run:no-build": "npm run typeorm -- migration:run -d src/dataSource.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/dataSource.ts",
    "migration:create": "npm run typeorm -- migration:create"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@bcgov/citz-imb-kc-express": "https://github.com/bcgov/citz-imb-kc-express/releases/download/v1.3.1/bcgov-citz-imb-kc-express-1.3.1.tgz",
    "cors": "2.8.5",
    "express": "4.18.2",
    "express-rate-limit": "7.1.5",
    "pg": "8.11.3",
    "reflect-metadata": "0.2.1",
    "typeorm": "0.3.19",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.4",
    "@types/pg": "8.10.9",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "nodemon": "3.0.2",
    "prettier": "3.2.2",
    "ts-node": "10.9.2",
    "tsx": "4.7.0",
    "typescript": "5.3.3"
  }
}
